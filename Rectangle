public class Rectangle {
    int length;
    int breadth;
    public static void Area()
    {
        int length=4;
        int breadth=5;
        System.out.println("Area of rectangle 1:- "+ length*breadth);
        length=5;
        breadth=8;
        System.out.println("Area of rectangle 2:- "+ length*breadth);
    }

    public static void main(String[] args) {
        Area();
    }


}
